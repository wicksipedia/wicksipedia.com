{
  "name": "wicksipedia.com-astro",
  "type": "module",
  "version": "5.5.1",
  "scripts": {
    "dev": "tinacms dev -c \"astro dev\"",
    "dev:astro": "astro dev",
    "build": "astro check && astro build && pagefind --site dist && cp -r dist/pagefind public/",
    "build:tina": "tinacms build && astro check && astro build && pagefind --site dist && cp -r dist/pagefind public/",
    "preview": "astro build && npx wrangler dev",
    "deploy": "npx wrangler deploy",
    "sync": "astro sync",
    "astro": "astro",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint": "eslint ."
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.12",
    "@astrojs/mdx": "^4.3.13",
    "@astrojs/react": "^4.4.2",
    "@astrojs/rss": "4.0.15",
    "@astrojs/sitemap": "3.7.0",
    "@resvg/resvg-js": "^2.6.2",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "astro": "5.16.15",
    "dayjs": "^1.11.19",
    "lodash.kebabcase": "^4.1.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "satori": "^0.18.3",
    "sharp": "^0.34.5",
    "slugify": "^1.6.6",
    "tailwindcss": "^4.1.18",
    "tinacms": "^3.4.1"
